{
  "name": "simple-chat-interface",
  "version": "1.0.0",
  "description": "A modern, responsive chat interface built with HTML, CSS, and JavaScript",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "backup": "node backup-database.js",
    "backup:list": "node backup-database.js --list",
    "backup:restore": "node backup-database.js --restore",
    "migrate": "node railway-migrate.js",
    "migrate:aurora": "node database/migrate.js",
    "debug:db": "node debug-railway-db.js"
  },
  "keywords": [
    "chat",
    "interface",
    "html",
    "css",
    "javascript",
    "web"
  ],
  "author": "Colin Davitian",
  "license": "MIT",
  "dependencies": {
    "@openai/agents": "^0.2.1",
    "@openai/chatkit-react": "^1.1.1",
    "@openai/guardrails": "^0.1.0",
    "amazon-cognito-identity-js": "^6.3.0",
    "aws-sdk": "^2.1490.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "openai": "^6.8.1",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@types/uuid": "^9.0.7",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
